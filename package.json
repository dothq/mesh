    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^5.3.0",
    "husky": "^4.3.8",
    "prettier": "2.2.1",
    "razzle": "^3.3.11",
    "tslint-config-prettier": "^1.18.0"
  }
