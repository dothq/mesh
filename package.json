  "license": "MIT",
  "scripts": {
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "start": "concurrently \"yarn start:tsc\" \"razzle start\"",
    "build": "tsc -b && razzle build",
    "start:prod": "NODE_ENV=production node build/server.js",
    "deploy": "yarn build && yarn run build:sitemap && vercel --prod",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:sitemap": "ts-node utils/generate-sitemap.ts"
  },
  "dependencies": {
    "@octokit/core": "^3.4.0",
    "@oieduardorabelo/use-user-agent": "^2.0.0",
    "@prisma/client": "^2.15.0",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/styled-components": "^5.1.7",
    "argon2": "^0.27.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crawler": "^1.3.0",
    "express": "^4.17.1",
    "markdown-to-jsx": "^7.1.1",
    "razzle-plugin-mdx": "^3.3.13",
    "razzle-plugin-typescript": "^3.3.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
